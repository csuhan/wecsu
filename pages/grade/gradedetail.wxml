<view class="header">{{grade.ClassName}}</view>

<view class="zan-panel">
  <view class="zan-cell">
    <view class="zan-cell__bd">过程成绩</view>
    <view class="zan-cell__ft">{{grade.MiddleGrade}}</view>
  </view>

  <view class="zan-cell">
    <view class="zan-cell__bd">期末成绩</view>
    <view class="zan-cell__ft">{{grade.FinalGrade}}</view>
  </view>

  <view class="zan-cell">
    <view class="zan-cell__bd">最终成绩</view>
    <view class="zan-cell__ft">{{grade.Grade}}</view>
  </view>

  <view class="zan-cell">
    <view class="zan-cell__bd">初修学期</view>
    <view class="zan-cell__ft">{{grade.FirstTerm}}</view>
  </view>

  <view class="zan-cell">
    <view class="zan-cell__bd">获得学期</view>
    <view class="zan-cell__ft">{{grade.GottenTerm}}</view>
  </view>
  <view class="zan-cell">
    <view class="zan-cell__bd">学分</view>
    <view class="zan-cell__ft">{{grade.ClassScore}}</view>
  </view>

  <view class="zan-cell">
    <view class="zan-cell__bd">课程类型</view>
    <view class="zan-cell__ft">{{grade.ClassType}}</view>
  </view>

  <view class="zan-cell">
    <view class="zan-cell__bd">课程性质</view>
    <view class="zan-cell__ft">{{grade.ClassProp}}</view>
  </view>
</view>